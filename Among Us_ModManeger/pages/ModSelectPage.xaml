<Page x:Class="Among_Us_ModManeger.Pages.ModSelectPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="clr-namespace:Among_Us_ModManeger.Pages"
      Title="ModSelectPage">

    <Grid Background="White" Margin="30">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <!-- 「.zipファイルから導入」ボタンを削除 -->

        <Button Grid.Row="1"
                Content="戻る"
                Width="100"
                Height="30"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Margin="0,20,0,0"
                Click="BackToMain_Click"/>
    </Grid>
</Page>
